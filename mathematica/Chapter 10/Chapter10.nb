Cell[TextData[{
 "The inverse operation of elementary subdivision is referred to as \
smoothing. To be precise, let ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]],ExpressionUUID->
  "40c1ffa6-111d-4f30-9f9e-9ad3b7a93b9f"],
 " be a vertex of degree 2 with neighbors ",
 Cell[BoxData[
  FormBox["u", TraditionalForm]],ExpressionUUID->
  "be57f002-1284-4dfc-a443-d86fc12f53c0"],
 " and ",
 Cell[BoxData[
  FormBox["w", TraditionalForm]],ExpressionUUID->
  "9bc798ba-4564-4deb-b22c-46b9f540896f"],
 " and such that ",
 Cell[BoxData[
  FormBox["u", TraditionalForm]],ExpressionUUID->
  "9414e331-3567-4b18-ad26-8cc598827397"],
 " and ",
 Cell[BoxData[
  FormBox["w", TraditionalForm]],ExpressionUUID->
  "eab73d01-e1df-4dc1-8d02-c56b1a9ef355"],
 " are not adjacent. We smooth the vertex ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]],ExpressionUUID->
  "c2c8b864-1768-412f-97d6-2bae16754a75"],
 " by deleting ",
 Cell[BoxData[
  FormBox["v", TraditionalForm]],ExpressionUUID->
  "ba4f72d6-9f12-42f8-92e9-95c8d46a24eb"],
 " and the edges incident to it and adding the edge ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"u", ",", "w"}], "}"}], TraditionalForm]],ExpressionUUID->
  "dfff140f-d7a8-4fbd-a810-50782e297fb7"],
 ". Below, we have created a function to implement smoothing. "
}], "Text",ExpressionUUID->"d765ac0d-895f-4287-99ef-d21eba4ff8c7"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"smoothGraph", "::", "vertx"}], "=", 
   "\"\<Cannot smooth this vertex.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"smoothGraph", "[", 
    RowBox[{"G_Graph", ",", "v_"}], "]"}], "/;", 
   RowBox[{"UndirectedGraphQ", "[", "G", "]"}]}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"N", ",", "H", ",", "e"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"N", "=", 
      RowBox[{"AdjacencyList", "[", 
       RowBox[{"G", ",", "v"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Length", "[", "N", "]"}], "\[NotEqual]", "2"}], "||", 
        RowBox[{"EdgeQ", "[", 
         RowBox[{"G", ",", 
          RowBox[{"UndirectedEdge", "[", 
           RowBox[{
            RowBox[{"N", "[", 
             RowBox[{"[", "1", "]"}], "]"}], ",", 
            RowBox[{"N", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], "]"}]}], ",", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{"smoothGraph", "::", "vertx"}], "]"}], ";", 
        RowBox[{"Return", "[", "$Failed", "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"H", "=", 
      RowBox[{"VertexDelete", "[", 
       RowBox[{"G", ",", "v"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"e", "=", 
      RowBox[{"UndirectedEdge", "[", 
       RowBox[{
        RowBox[{"N", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{"N", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"H", "=", 
      RowBox[{"EdgeAdd", "[", 
       RowBox[{"H", ",", "e"}], "]"}]}], ";", "\[IndentingNewLine]", "H"}]}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
 InitializationCell->True,
 CellLabel->
  "In[242]:=",ExpressionUUID->"fa505704-dc1c-4a32-be58-75eea4f2bce9"],

