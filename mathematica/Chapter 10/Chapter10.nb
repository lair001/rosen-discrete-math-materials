Cell["\<\
Display a full set of nonisomorphic simple graphs with six vertices.\
\>", "Problem",ExpressionUUID->"8ff022e5-18a9-4e3f-8e32-6e9430a74522"],

Cell[TextData[{
 StyleBox["Solution:",
  FontSlant->"Italic"],
 " The solution to this exercise is very similar to the previous question. \
The only difference is that, once the list of graphs is generated, we remove \
those that are isomorphic to others by applying ",
 StyleBox[ButtonBox["DeleteDuplicates",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/DeleteDuplicates"], "FunctionLink"],
 " with second argument ",
 StyleBox[ButtonBox["IsomorphicGraphQ",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/IsomorphicGraphQ"], "FunctionLink"],
 ". This uses ",
 StyleBox[ButtonBox["IsomorphicGraphQ",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/IsomorphicGraphQ"], "FunctionLink"],
 " to determine whether two elements are to be considered duplicates or not."
}], "Text",ExpressionUUID->"ca919fa7-5f65-49c0-84a4-62743f36372d"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"nonIsoGraphs", "[", "n_Integer", "]"}], "/;", 
   RowBox[{"n", ">", "0"}]}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"cg", ",", "v", ",", 
      RowBox[{"A", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"V", "=", 
       RowBox[{"Range", "[", "n", "]"}]}], ",", "powerE", ",", "vCoords", ",",
       "E", ",", "i", ",", "G", ",", "j"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"cg", "=", 
      RowBox[{"CompleteGraph", "[", "n", "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"powerE", "=", 
      RowBox[{"Subsets", "[", 
       RowBox[{"EdgeList", "[", "cg", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"vCoords", "=", 
      RowBox[{"GraphEmbedding", "[", "cg", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"AppendTo", "[", 
        RowBox[{"A", ",", 
         RowBox[{"Graph", "[", 
          RowBox[{"V", ",", "E", ",", 
           RowBox[{"VertexCoordinates", "\[Rule]", "vCoords"}]}], "]"}]}], 
        "]"}], "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"E", ",", "powerE"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"DeleteDuplicates", "[", 
      RowBox[{"A", ",", "IsomorphicGraphQ"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
 CellLabel->
  "In[256]:=",ExpressionUUID->"a27180cd-6386-4dc3-9fa6-21e70885f062"],

