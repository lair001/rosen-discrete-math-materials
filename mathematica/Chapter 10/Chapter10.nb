Cell["Based on this example, we create the following function.", "Text",ExpressionUUID->"e6e69d3e-d91c-44d1-9378-11ae272a41d3"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"subdivideGraph", "::", "nonedge"}], "=", 
   "\"\<Second argument must be an edge in the graph.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"subdivideGraph", "[", 
    RowBox[{"G_Graph", ",", 
     RowBox[{"E_Rule", "|", "E_UndirectedEdge"}]}], "]"}], "/;", 
   RowBox[{"UndirectedGraphQ", "[", "G", "]"}]}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"H", ",", "e", ",", "newV", ",", "v"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Head", "[", "E", "]"}], "===", "Rule"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"e", "=", 
        RowBox[{"UndirectedEdge", "@@", "E"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"e", "=", "E"}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"!", 
        RowBox[{"EdgeQ", "[", 
         RowBox[{"G", ",", "e"}], "]"}]}], ",", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{"subdivideGraph", "::", "nonedge"}], "]"}], ";", 
        RowBox[{"Return", "[", "$Failed", "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"newV", "=", 
      RowBox[{
       RowBox[{"ToString", "[", 
        RowBox[{"e", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "]"}], "<>", "\"\<-\>\"", "<>", 
       RowBox[{"ToString", "[", 
        RowBox[{"e", "[", 
         RowBox[{"[", "2", "]"}], "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"H", "=", 
      RowBox[{"Graph", "[", 
       RowBox[{
        RowBox[{"VertexAdd", "[", 
         RowBox[{"G", ",", "newV"}], "]"}], ",", 
        RowBox[{"VertexCoordinates", "\[Rule]", 
         RowBox[{"Append", "[", 
          RowBox[{
           RowBox[{"GraphEmbedding", "[", "G", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", "0"}], "}"}]}], "]"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"PropertyValue", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"H", ",", "newV"}], "}"}], ",", "VertexCoordinates"}], "]"}],
       "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"PropertyValue", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"H", ",", 
             RowBox[{"e", "[", 
              RowBox[{"[", "1", "]"}], "]"}]}], "}"}], ",", 
           "VertexCoordinates"}], "]"}], "+", 
         RowBox[{"PropertyValue", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"H", ",", 
             RowBox[{"e", "[", 
              RowBox[{"[", "2", "]"}], "]"}]}], "}"}], ",", 
           "VertexCoordinates"}], "]"}]}], ")"}], "/", "2"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"H", "=", 
      RowBox[{"EdgeDelete", "[", 
       RowBox[{"H", ",", "e"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"H", "=", 
      RowBox[{"EdgeAdd", "[", 
       RowBox[{"H", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"e", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", "newV"}], ",", 
          RowBox[{"newV", "\[Rule]", 
           RowBox[{"e", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}]}], "}"}]}], "]"}]}], ";", 
