Cell[BoxData[
 RowBox[{
  RowBox[{"drawBipartite", "[", "g_Graph", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"V", ",", "AB", ",", "i", ",", "T", ",", "w", ",", "e"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"V", "=", 
      RowBox[{"VertexList", "[", "g", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"w", "=", 
      RowBox[{"V", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"AB", "[", "0", "]"}], "=", 
      RowBox[{"{", "w", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"AB", "[", "1", "]"}], "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"i", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"V", "\[NotEqual]", 
        RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"T", "=", 
         RowBox[{"Intersection", "[", 
          RowBox[{"V", ",", 
           RowBox[{"AB", "[", "i", "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"i", "=", 
         RowBox[{"Mod", "[", 
          RowBox[{
           RowBox[{"i", "+", "1"}], ",", "2"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"AB", "[", "i", "]"}], "=", 
           RowBox[{"Union", "[", 
            RowBox[{
             RowBox[{"AB", "[", "i", "]"}], ",", 
             RowBox[{"Complement", "[", 
              RowBox[{
               RowBox[{"neighbors", "[", 
                RowBox[{"g", ",", "w"}], "]"}], ",", 
               RowBox[{"Union", "[", 
                RowBox[{
                 RowBox[{"AB", "[", "0", "]"}], ",", 
                 RowBox[{"AB", "[", "1", "]"}]}], "]"}]}], "]"}]}], "]"}]}], 
          "\[IndentingNewLine]", ",", 
          RowBox[{"{", 
           RowBox[{"w", ",", "T"}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"V", "=", 
         RowBox[{"Complement", "[", 
          RowBox[{"V", ",", "T"}], "]"}]}]}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Catch", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Do", "[", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"MemberQ", "[", 
               RowBox[{
                RowBox[{"AB", "[", "0", "]"}], ",", 
                RowBox[{"e", "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "&&", 
              RowBox[{"MemberQ", "[", 
               RowBox[{
                RowBox[{"AB", "[", "0", "]"}], ",", 
                RowBox[{"e", "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ")"}], "||", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"MemberQ", "[", 
               RowBox[{
                RowBox[{"AB", "[", "1", "]"}], ",", 
                RowBox[{"e", "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "&&", 
              RowBox[{"MemberQ", "[", 
               RowBox[{
                RowBox[{"AB", "[", "1", "]"}], ",", 
                RowBox[{"e", "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ")"}]}], ",", 
           RowBox[{"Throw", "[", "False", "]"}]}], "]"}], 
         "\[IndentingNewLine]", ",", 
         RowBox[{"{", 
          RowBox[{"e", ",", 
           RowBox[{"EdgeList", "[", "g", "]"}]}], "}"}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Graph", "[", 
        RowBox[{"g", ",", 
         RowBox[{"VertexStyle", "\[Rule]", 
          RowBox[{"Union", "[", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"i", "\[Rule]", "Green"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"AB", "[", "0", "]"}]}], "}"}]}], "]"}], ",", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"i", "\[Rule]", "Yellow"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"AB", "[", "1", "]"}]}], "}"}]}], "]"}]}], "]"}]}], 
         ",", 
         RowBox[{"VertexShapeFunction", "\[Rule]", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"i", "\[Rule]", "\"\<Square\>\""}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", 
              RowBox[{"AB", "[", "0", "]"}]}], "}"}]}], "]"}]}]}], "]"}]}], 
