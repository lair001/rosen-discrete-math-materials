 "."
}], "Text",ExpressionUUID->"3fb073bf-817d-44e1-9acd-39dceba67b1c"],

Cell[TextData[{
 "For convenience, we define a version of ",
 StyleBox["drawBipartite", "Code"],
 " that can accept additional ",
 StyleBox[ButtonBox["Graph",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Graph"], "FunctionLink"],
 " options and pass them through our function to where we create the new ",
 StyleBox[ButtonBox["Graph",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Graph"], "FunctionLink"],
 ". The easiest way to do this is to add a ",
 StyleBox[ButtonBox["BlankNullSequence",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/BlankNullSequence"], "FunctionLink"],
 " (",
 StyleBox["___", "Code"],
 ") to the arguments of our function. The result is that the function \
requires a ",
 StyleBox[ButtonBox["Graph",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Graph"], "FunctionLink"],
 " as the first argument, but can accept any number of additional arguments \
or no additional arguments. We then just pass these to ",
 StyleBox[ButtonBox["Graph",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Graph"], "FunctionLink"],
 "."
}], "Text",ExpressionUUID->"a10c8912-7be5-46cc-ab27-64d825c1bf57"],

Cell[BoxData[
 RowBox[{
  RowBox[{"drawBipartite", "[", 
   RowBox[{"g_Graph", ",", "opts___"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"V", ",", "AB", ",", "i", ",", "T", ",", "w", ",", "e"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"V", "=", 
      RowBox[{"VertexList", "[", "g", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"w", "=", 
      RowBox[{"V", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"AB", "[", "0", "]"}], "=", 
      RowBox[{"{", "w", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"AB", "[", "1", "]"}], "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"i", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"V", "\[NotEqual]", 
        RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"T", "=", 
         RowBox[{"Intersection", "[", 
          RowBox[{"V", ",", 
           RowBox[{"AB", "[", "i", "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"i", "=", 
         RowBox[{"Mod", "[", 
          RowBox[{
           RowBox[{"i", "+", "1"}], ",", "2"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"AB", "[", "i", "]"}], "=", 
           RowBox[{"Union", "[", 
            RowBox[{
             RowBox[{"AB", "[", "i", "]"}], ",", 
             RowBox[{"Complement", "[", 
              RowBox[{
               RowBox[{"neighbors", "[", 
                RowBox[{"g", ",", "w"}], "]"}], ",", 
               RowBox[{"Union", "[", 
                RowBox[{
                 RowBox[{"AB", "[", "0", "]"}], ",", 
                 RowBox[{"AB", "[", "1", "]"}]}], "]"}]}], "]"}]}], "]"}]}], 
          "\[IndentingNewLine]", ",", 
          RowBox[{"{", 
           RowBox[{"w", ",", "T"}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"V", "=", 
         RowBox[{"Complement", "[", 
          RowBox[{"V", ",", "T"}], "]"}]}]}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Catch", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Do", "[", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"MemberQ", "[", 
               RowBox[{
                RowBox[{"AB", "[", "0", "]"}], ",", 
                RowBox[{"e", "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "&&", 
              RowBox[{"MemberQ", "[", 
               RowBox[{
                RowBox[{"AB", "[", "0", "]"}], ",", 
                RowBox[{"e", "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ")"}], "||", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"MemberQ", "[", 
               RowBox[{
                RowBox[{"AB", "[", "1", "]"}], ",", 
                RowBox[{"e", "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "&&", 
              RowBox[{"MemberQ", "[", 
               RowBox[{
                RowBox[{"AB", "[", "1", "]"}], ",", 
                RowBox[{"e", "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ")"}]}], ",", 
           RowBox[{"Throw", "[", "False", "]"}]}], "]"}], 
         "\[IndentingNewLine]", ",", 
         RowBox[{"{", 
          RowBox[{"e", ",", 
           RowBox[{"EdgeList", "[", "g", "]"}]}], "}"}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Graph", "[", 
        RowBox[{"g", ",", 
         RowBox[{"VertexStyle", "\[Rule]", 
          RowBox[{"Union", "[", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"i", "\[Rule]", "Green"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"AB", "[", "0", "]"}]}], "}"}]}], "]"}], ",", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"i", "\[Rule]", "Yellow"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"AB", "[", "1", "]"}]}], "}"}]}], "]"}]}], "]"}]}], 
         ",", 
         RowBox[{"VertexShapeFunction", "\[Rule]", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"i", "\[Rule]", "\"\<Square\>\""}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", 
              RowBox[{"AB", "[", "0", "]"}]}], "}"}]}], "]"}]}], ",", 
         "opts"}], "]"}]}], "\[IndentingNewLine]", "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
 CellLabel->"In[82]:=",ExpressionUUID->"962bfc11-e098-4549-a16b-b6dc27286eea"],

