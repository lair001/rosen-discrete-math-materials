    RowBox[{
     RowBox[{"DirectedEdge", "[", 
      RowBox[{"3", ",", "x_"}], "]"}], "|", 
     RowBox[{"DirectedEdge", "[", 
      RowBox[{"x_", ",", "3"}], "]"}], "|", 
     RowBox[{"UndirectedEdge", "[", 
      RowBox[{"3", ",", "x_"}], "]"}], "|", 
     RowBox[{"UndirectedEdge", "[", 
      RowBox[{"x_", ",", "3"}], "]"}]}], "\[RuleDelayed]", "x"}]}], 
  "]"}]], "Input",
 CellLabel->"In[73]:=",ExpressionUUID->"bdefbebd-2802-45d4-ab3d-7b2aa66c5a79"],

