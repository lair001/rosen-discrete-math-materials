Cell[TextData[{
 "We now build ",
 StyleBox["adjacencyListGraph", "Code"],
 ". We use a ",
 StyleBox[ButtonBox["BlankNullSequence",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/BlankNullSequence"], "FunctionLink"],
 " (",
 StyleBox["___", "Code"],
 "), which matches any number of argument, including 0, to pass options from \
this function to ",
 StyleBox[ButtonBox["AdjacencyGraph",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/AdjacencyGraph"], "FunctionLink"],
 "."
}], "Text",ExpressionUUID->"0e19baf2-5173-4d06-a71c-e6fa2e0c5974"],

Cell[BoxData[
 RowBox[{
  RowBox[{"adjacencyListGraph", "[", 
   RowBox[{
    RowBox[{"A", ":", 
     RowBox[{"Association", "[", 
      RowBox[{"__", "?", "adjacencyRuleQ"}], "]"}]}], ",", "opts___"}], "]"}],
   ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"V", ",", "n", ",", "D", ",", "rules", ",", "v", ",", "w"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"V", "=", 
      RowBox[{"Union", "[", 
       RowBox[{
        RowBox[{"Keys", "[", "A", "]"}], ",", 
        RowBox[{"Flatten", "@", 
         RowBox[{"Values", "[", "A", "]"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"n", "=", 
      RowBox[{"Length", "[", "V", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"D", "=", 
      RowBox[{"Association", "[", 
       RowBox[{"MapThread", "[", 
        RowBox[{"Rule", ",", 
         RowBox[{"{", 
          RowBox[{"V", ",", 
           RowBox[{"Range", "[", "n", "]"}]}], "}"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rules", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"D", "[", "v", "]"}], ",", 
            RowBox[{"D", "[", "w", "]"}]}], "}"}], "\[Rule]", "1"}], ",", 
         RowBox[{"{", 
          RowBox[{"v", ",", 
           RowBox[{"Keys", "[", "A", "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"w", ",", 
           RowBox[{"A", "[", "v", "]"}]}], "}"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"AdjacencyGraph", "[", 
      RowBox[{
       RowBox[{"SparseArray", "[", 
        RowBox[{"rules", ",", 
         RowBox[{"{", 
          RowBox[{"n", ",", "n"}], "}"}]}], "]"}], ",", "opts"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
 CellLabel->
  "In[142]:=",ExpressionUUID->"bfd71859-adff-45db-8331-d00d901ec524"],

