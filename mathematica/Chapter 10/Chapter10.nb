Cell[TextData[{
 "We now turn this into a function. The only difference between the ",
 StyleBox["animatePath", "Code"],
 " function below and the example from above is that we replace the variable \
in the loop specification with the list ",
 StyleBox["{i,\[ThinSpace]0,\[ThinSpace]\"step\"}", "Code"],
 ". This syntax gives the variable ",
 StyleBox["i", "Code"],
 " the initial value 0 and labels it as \[OpenCurlyDoubleQuote]step\
\[CloseCurlyDoubleQuote] in the animation controller. "
}], "Text",ExpressionUUID->"5d9a9938-ee9c-4146-8cdd-a088536edef9"],

Cell[BoxData[
 RowBox[{
  RowBox[{"animatePath", "[", 
   RowBox[{"g_Graph", ",", "p_List"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i", ",", "len"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"len", "=", 
      RowBox[{"Length", "[", "p", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Animate", "[", 
      RowBox[{
       RowBox[{"HighlightGraph", "[", 
        RowBox[{"g", ",", 
         RowBox[{"p", "[", 
          RowBox[{"[", 
           RowBox[{"1", ";;", "i"}], "]"}], "]"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"i", ",", "0", ",", "\"\<step\>\""}], "}"}], ",", "0", ",", 
         "len", ",", "1"}], "}"}], ",", 
       RowBox[{"AnimationRunning", "\[Rule]", "False"}], ",", 
       RowBox[{"AnimationRepetitions", "\[Rule]", "1"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
 CellLabel->
  "In[194]:=",ExpressionUUID->"615a8938-dc74-4988-ac6c-ab2570f08891"],

