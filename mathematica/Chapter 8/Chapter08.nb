           RowBox[{"[", "2", "]"}], "]"}]}], "&"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p", "=", 
      RowBox[{"compatible", "[", "L", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"T", "=", 
      RowBox[{"totalAttendance", "[", 
       RowBox[{"L", ",", "p"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"T", "[", 
      RowBox[{"Length", "[", "talkList", "]"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
 CellLabel->"In[18]:=",ExpressionUUID->"3ad7dce0-6cf3-4edd-93f7-d39cfdc212dc"],

