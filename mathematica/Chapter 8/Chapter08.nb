   {
    RowBox[{"Start", " ", "Time"}], 
    RowBox[{"End", " ", "Time"}], "Attendance"},
   {
    RowBox[{"9", ":", 
     RowBox[{"00", " ", "AM"}]}], 
    RowBox[{"11", ":", 
     RowBox[{"00", " ", "AM"}]}], "17"},
