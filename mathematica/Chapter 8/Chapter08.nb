Cell[CellGroupData[{

Cell["A Second Example", "Subsubsection",ExpressionUUID->"37b11016-7e92-45ba-9107-b5ff8212b057"],

Cell["\<\
We consider another example. We will solve the recurrence relation\
\>", "Text",ExpressionUUID->"0d854eda-871d-4bcf-b2ba-d98e9c152aad"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["a", "n"], "=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"-", "5"}], "3"], 
     SubscriptBox["a", 
      RowBox[{"n", "-", "1"}]]}], "+", 
    RowBox[{
     FractionBox["2", "3"], 
     SubscriptBox["a", 
      RowBox[{"n", "-", "2"}]]}]}]}], TraditionalForm]], "Equation",Expression\
UUID->"a460cb23-d45e-460e-ae93-2ce712dc56ce"],

Cell[TextData[{
 "with initial conditions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "1"], "=", 
    FractionBox["1", "2"]}], TraditionalForm]],ExpressionUUID->
  "3a8b5b60-dcca-4dad-bbcd-a3c28c7a5a19"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "2"], "=", "4"}], TraditionalForm]],ExpressionUUID->
  "a24f3710-8ebf-4681-ade9-a801c1b8dc28"],
 "."
}], "Text",ExpressionUUID->"3d932fc2-e641-48ed-84c1-ef86ffe2b391"],

Cell[TextData[{
 "To do this, we have ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " solve the characteristic equation of the recurrence relation, and then \
solve the system of linear equations obtained from the roots of the \
characteristic equation and the initial conditions. Note that this method \
works because this recurrence relation is linear, homogeneous, and has \
constant coefficients."
}], "Text",ExpressionUUID->"e16d0948-5f50-4585-96ba-30bb4dc1153a"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"charEqnRoots", "=", 
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"r", "^", "2"}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"5", "/", "3"}], ")"}], "*", "r"}], "-", 
      RowBox[{"(", 
       RowBox[{"2", "/", "3"}], ")"}]}], "\[Equal]", "0"}], ",", "r"}], 
   "]"}]}]], "Input",
 InitializationCell->True,
 CellLabel->"In[24]:=",ExpressionUUID->"d4f6d575-8021-48c1-b227-4ae1349db655"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"r", "\[Rule]", 
     RowBox[{"-", "2"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"r", "\[Rule]", 
     FractionBox["1", "3"]}], "}"}]}], "}"}]], "Output",
 CellLabel->"Out[24]=",ExpressionUUID->"b1ca4e7c-ca71-44af-bcb7-1261e76a4c22"]
}, Open  ]],

Cell[TextData[{
