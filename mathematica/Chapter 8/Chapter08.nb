 CellLabel->"In[92]:=",ExpressionUUID->"ec5a605a-8bb8-4e7d-a701-2515d1cd8e52"],

Cell[BoxData[
 FractionBox["1", 
  RowBox[{"1", "-", 
   SuperscriptBox["x", "5"]}]]], "Output",
 CellLabel->"Out[92]=",ExpressionUUID->"49340987-293c-4180-949f-1b1ff390999d"]
}, Open  ]],

Cell[TextData[{
 "We can use ",
 StyleBox[ButtonBox["Series",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Series"], "FunctionLink"],
 " to confirm that ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " does in fact recognize this as the generating function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "+", 
    SuperscriptBox["x", "5"], "+", 
    SuperscriptBox["x", "10"], "+", 
    SuperscriptBox["x", "15"], "+", "\[CenterEllipsis]"}], TraditionalForm]],
  ExpressionUUID->"2900960b-53a2-4d30-8161-c75e48599e4b"],
 "."
}], "Text",ExpressionUUID->"530da518-fb80-4207-80f4-c04a531538da"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Series", "[", 
  RowBox[{"token5D", ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "0", ",", "20"}], "}"}]}], "]"}]], "Input",
 CellLabel->"In[93]:=",ExpressionUUID->"d7c9d6b2-58a3-4c13-b697-cb9c353907cb"],

Cell[BoxData[
 InterpretationBox[
