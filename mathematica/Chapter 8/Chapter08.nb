Cell["A More General Recurrence Solver", "Subsubsection",ExpressionUUID->"ff0af02b-213b-4fe6-bb1f-18d29c855458"],

Cell[TextData[{
 "The steps carried out above are quite general and we can write a function, ",
 StyleBox["recSolver2", "Code"],
 ", which solves a recurrence relation (degree two, linear, homogeneous, with \
constant coefficients) regardless of whether the characteristic polynomial \
has distinct roots or not. The following function solves the recurrence"
}], "Text",ExpressionUUID->"7ca1f596-ba81-4e15-97ab-43a7350331be"],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["a", "n"], "=", 
   RowBox[{
    RowBox[{"c", "\[CenterDot]", 
     SubscriptBox["a", 
      RowBox[{"n", "-", "1"}]]}], "+", 
    RowBox[{"d", "\[CenterDot]", 
     SubscriptBox["a", 
      RowBox[{"n", "-", "2"}]]}]}]}], TraditionalForm]], "Equation",Expression\
UUID->"fffc08dd-b4f5-4afb-8ce0-8ccd535e6c6f"],

Cell[TextData[{
 "with initial conditions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "1"], "=", "u"}], TraditionalForm]],ExpressionUUID->
  "927835a1-bde3-41ec-811d-6cdc3b487b9e"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "2"], "=", "v"}], TraditionalForm]],ExpressionUUID->
  "d57fd46d-b8c6-4ae3-bdcc-94e9db362fc7"],
 "."
}], "Text",ExpressionUUID->"3faca974-3cf0-403f-8d1b-5557722aaa11"],

Cell[BoxData[
 RowBox[{
  RowBox[{"recSolver2", "[", 
   RowBox[{"c_", ",", "d_", ",", "u_", ",", "v_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"CERoots", ",", "alphas", ",", "alpha", ",", "beta", ",", "r"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "first", " ", "solve", " ", "the", " ", "characteristic", " ", 
      "equation"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"CERoots", "=", 
      RowBox[{"r", "/.", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"r", "^", "2"}], "-", 
           RowBox[{"c", "*", "r"}], "-", "d"}], "\[Equal]", "0"}], ",", "r"}],
         "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "then", " ", "test", " ", "if", " ", "the", " ", "roots", " ", "are", 
       " ", "the", " ", "same"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"CERoots", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "\[Equal]", 
        RowBox[{"CERoots", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"the", " ", "roots", " ", "are", " ", "the", " ", "same"}], 
         ",", " ", 
         RowBox[{"follow", " ", "the", " ", "last", " ", "example"}]}], " ", 
        "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"alphas", "=", 
         RowBox[{
          RowBox[{"Solve", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"alpha", "*", 
                 RowBox[{"CERoots", "[", 
                  RowBox[{"[", "1", "]"}], "]"}]}], "+", 
                RowBox[{"beta", "*", 
                 RowBox[{"CERoots", "[", 
                  RowBox[{"[", "1", "]"}], "]"}]}]}], "\[Equal]", "u"}], ",", 
              
              RowBox[{
               RowBox[{
                RowBox[{"alpha", "*", 
                 RowBox[{
                  RowBox[{"CERoots", "[", 
                   RowBox[{"[", "1", "]"}], "]"}], "^", "2"}]}], "+", 
                RowBox[{"2", "*", "beta", "*", 
                 RowBox[{
                  RowBox[{"CERoots", "[", 
                   RowBox[{"[", "1", "]"}], "]"}], "^", "2"}]}]}], "\[Equal]",
                "v"}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"alpha", ",", "beta"}], "}"}]}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Return", "[", 
         RowBox[{"Function", "[", 
          RowBox[{"n", ",", 
           RowBox[{
            RowBox[{
             RowBox[{"alpha", "*", 
              RowBox[{
               RowBox[{"CERoots", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "^", "n"}]}], "+", 
             RowBox[{"n", "*", "beta", "*", 
              RowBox[{
               RowBox[{"CERoots", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "^", "n"}]}]}], "/.", 
            "alphas"}]}], "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"otherwise", ",", " ", 
         RowBox[{"use", " ", "the", " ", "recSol2Distinct", " ", "method"}]}],
         " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"alphas", "=", 
         RowBox[{
          RowBox[{"Solve", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"alpha", "*", 
                 RowBox[{"CERoots", "[", 
